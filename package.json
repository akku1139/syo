{
  "name": "syo",
  "version": "0.2.0",
  "description": "Cool and Easy documentation site generator",
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akku1139/syo.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "docs": "cd docs && node ../bin/syo.mjs",
    "test": "pnpm run typecheck && pnpm run test:basic",
    "test:basic": "node --test",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "bin": "./bin/syo.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "@farmfe/core": "^1.7.1",
    "@shikijs/twoslash": "^3.2.1",
    "@solidjs/router": "^0.15.3",
    "marked": "^15.0.7",
    "shiki": "^3.2.1",
    "solid-js": "^1.9.5",
    "valibot": "1.0.0-rc.4",
    "vite-plugin-solid": "^2.11.6",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "typescript": "^5.8.2"
  }
}
